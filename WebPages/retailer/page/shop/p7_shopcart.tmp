						        		 	$("a.shop-select").children("div").addClass("icon-selected");
						        		 }
						            	totalPrices();
					       			 }
				    			});//编辑购物车选中状态	结束
				    			
								//计算商品总价	
								function  totalPrices()
								{
							         var Totalprices=0;  //总价
							         var 	pricelen=0;  
							         var div_len=$("div.order-select .icon.icon-selected").length//商品数量
							        $(".grid-shop").find("div.order-select .icon.icon-selected").each(function (i)
							        {
							        	
										var   $_this =$(this);
							            var len=$_this.length;
							             pricelen +=len;
										var count=$(this).parent().parent().next().next().find(".dis").html();    
										var  countlen=count.length;
										var cou=Number(count.substring(1,countlen)) 
										//获取商品数量
										var p=$(this).parent().parent().next().next().find(".price").html();
										var plen=p.length;
										var pri=Number(p.substring(0,plen-3)); 
										//获取商品价格
										Totalprices+=cou*pri
							        });
							        
									$(".shopcartammount").html('('+pricelen+')')  
									$(".sum1").html('('+pricelen+')')     //显示购物车数量
									str='合计：￥'+Totalprices+'元'         
									$("#R_Main").find(".sum").html(str);               //显示总价
							    }
							    totalPrices()
							    
							    //提交
								$(".bottomsubmit").click(function ()
								{
									var str='[';
									$(".orderlayout").find("div.order-select .icon.icon-selected").each(function (i)
									{
									 	   itemid_=$(this).find(".hidden_itemid").val();
									 	var   isyuncun_=$(this).find(".hidden_isyucun").val();
									 	var   activityitem_id_=$(this).find(".hidden_activityitem_id").val();
										str+='{"itemid":"'+itemid_+'","isyucun":"'+isyuncun_+'","activityitem_id":"'+activityitem_id_+'"},';
								 	});
								 	str = str.substring(0,str.length-1);
								 	str+=']';
							 		urlinfo="p12_orderconfirm.html?a="+str+"=distributor_id="+distributor_id;
									encodeURI(urlinfo);									
									window.location.href=urlinfo;
								});
							   //提交结束
								
		   			    }//success方法的结束
		
			    })//ajax结束
					
		 })// document结束
 
 	</script>
 
</head>

